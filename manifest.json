{
  "id": "plugin-chaos",
  "displayName": "Chaos",
  "description": "Causes defined record read errors.",
  "apiVersion": "2",
  "versionDescription": "Version 2",
  "version": "2.0.0",
  "iconFile": "icon.jpg",
  "executable": "PluginChaos",
  "kind": "publisher",
  "os": "$GOOS",
  "arch": "$GOARCH",
  "canConfigure": true,
  "canProduceMultipleSchemas": true,
  "canAcceptQueryBasedSchemas": false,
  "canUseOAuth": false,
  "canWrite": true,
  "configSchema": {
    "ui": {
      "ui:order": [
        "ApiKey"
      ],
      "ApiKey": {
        "ui:widget": "password"
      }
    },
    "schema": {
      "type": "object",
      "properties": {
        "ApiKey" : {
          "type": "string",
          "title": "API Key",
          "description": "API key to use for authentication."
        }
      },
      "required": [
      ]
    }
  }
}